sudo vim /etc/nginx/sites-available/dudic

server {
	listen 80;
	server_name http://course.dudic.io/;
	client_max_body_size 100M;

	access_log /home/dic/workplace-djangoFrontend/dudic/logs/nginx-access.log;
	error_log /home/dic/workplace-djangoFrontend/dudic/logs/nginx-error.log;

	location /static/ {
		alias	/home/dic/workplace-djangoFrontend/dudic/static/;
        alias	/home/dic/workplace-djangoFrontend/dudic/staticfiles/;
	}

	location / {
		include proxy_params;
		proxy_pass http://unix:/home/dic/workplace-djangoFrontend/dudic/run/gunicorn_main.sock;
	}

}


